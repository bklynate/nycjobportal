{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "12.14.1"
  },
  "scripts": {
    "start": "npm run prod:buildserver; node ./server/server-dist/",
    "upgrade-interactive": "npm-check --update",
    "server": "nodemon ./server/index.js --exec babel-node",
    "prod:buildserver": "rm -rf ./server/server-dist && babel ./server -d ./server/server-dist --copy-files",
    "client": "npm run start --prefix client",
    "audit": "npm audit fix && cd client/ && npm audit fix && cd ..",
    "getAll": "npm i && cd client/ && npm i && cd ..",
    "dev": "concurrently --raw --kill-others \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "lint": "eslint \"./**/*.{js,jsx}\" --quiet",
    "test": "jest",
    "precommit": "lint-staged"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "babel-preset-react": "6.24.1",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "cookie-session": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "express": "^4.17.1",
    "mongoose": "^5.8.11",
    "nodemon": "^2.0.2",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "server/**/*.{js,jsx}"
    ],
    "testMatch": [
      "<rootDir>/server/**/**/__tests__/**/*.js?(x)",
      "<rootDir>/server/**/**/?(*.)(spec|test).js?(x)",
      "<rootDir>/server/**/?(*.)(spec|test).js"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.2",
    "jest": "^24.0.0",
    "npm-check": "^5.9.0",
    "prettier": "^1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testMatch": [
      "<rootDir>/server/**/**/__tests__/**/*.js?(x)",
      "<rootDir>/server/**/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
